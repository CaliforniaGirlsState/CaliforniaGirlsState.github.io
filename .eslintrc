{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb",
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "rules": {
    "no-console": "off",
    "react/jsx-filename-extension": "off",
    "react/forbid-prop-types": "off",
  }
}
